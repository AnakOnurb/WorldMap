<?xml version='1.0' encoding='utf-8'?>
<widget id="castellani.WorldMap" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>WorldMap</name>
    <description>
        World Info with World Bank API
    </description>
    <author email="bruno-castellani@hotmail.com" href="https://github.com/LittleCast">
        Castellani
    </author>
    <content src="index.html" />
    <icon density="mdpi" height="57" platform="android" src="www/src/head.png" width="57" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
